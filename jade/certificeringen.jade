extends default
block header
	- var pageTitle = 'Master it Training - 650 IT trainingen en cursussen';
	- var bodyClass = 'certificeringen';
block content
	+row('breadcrumbs')
		.col--breadcrumbs
			ol.breadcrumbs
				li: a(href="") Home
				li: a.active(href="") Certificeringen
			ul.in-page-nav
				li: a(href="") Microsoft
				li: a(href="") Citrix
				li: a(href="") Cisco
				li: a(href="") CompTIA
				li: a(href="") Linux
				li: a(href="") VMWare
	.outer--mobile-header-image
		.mobile-header-image
			img(src="../img/BB_140616MASTER-4594W-1600x444px.jpg")				
	+row('page-header')
		.col--page-header
			.page-header
				h1 Certificeringen
	+row('page-intro')
		.col--page-intro
			.page-intro
				.row
					.col--intro__text
						.intro__text
							p Hier komt de introductietekst met SEO-optimized termen voor deze pagina. Conseratur autempe libus. Biscid utem fuga. Ilitiis 
								span.hide-on-mobile eaquidio corum il ium quo molor maioratquis as pa non nonseque. Em ipsunt fugitiust vid eario. Hendige niminct inveles nus iligendiatis vollorem soloritatiam aut ma non pa nossum atio. Imolori onsendunt ut aut eatempo reressi magnient magni cum nim idel ipit ute seditas aspe volendae poritatem corum volorum endaes dit venimi.
					.col--intro__usp
						.intro__usp
							h2 Haal je certificaat met de Master it Certificeringspas
							ul
								li Eén voordelige pas voor alle trainingen en examens
								li Kies zelf wanneer en waar je wilt trainen
								li Binnen een jaar ben je gecertificeerd!
						.intro__links
							ul
								li: a(href="") Onze trainers
								li: a(href="") Onze methode
								li: a(href="") Erkenningen
								li: a(href="") Referenties						
